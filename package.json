{
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "deduplicate": "yarn-deduplicate",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx --quiet src",
    "test": "vitest --run --passWithNoTests",
    "test:watch": "vitest",
    "clean": "rimraf dist",
    "coverage": "vitest run --coverage",
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "release": "standard-version",
    "deploy": "./deploy.sh"
  },
  "husky": {
    "hooks": {
      "pre-commit": "run-s lint test",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "dependencies": {
    "@blackglory/prelude": "^0.2.0",
    "@blackglory/structures": "^0.11.8",
    "@dimforge/rapier2d-compat": "^0.10.0",
    "@headlessui/react": "^1.7.7",
    "bitecs": "^0.3.38",
    "box2d-wasm": "^7.0.0",
    "classnames": "^2.3.2",
    "extra-defer": "^0.2.7",
    "extra-ecs": "^0.3.4",
    "extra-game-loop": "^0.3.2",
    "extra-key-state": "^0.1.2",
    "extra-rand": "^0.1.1",
    "extra-react-hooks": "^0.3.3",
    "pixi.js": "^7.0.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "screenfull": "^6.0.2",
    "structure-of-arrays": "^0.5.12"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.3.0",
    "@commitlint/config-conventional": "^17.3.0",
    "@testing-library/dom": "^8.19.1",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.10",
    "@typescript-eslint/eslint-plugin": "^5.47.1",
    "@typescript-eslint/parser": "^5.47.1",
    "@vitejs/plugin-react": "^3.0.0",
    "autoprefixer": "^10.4.13",
    "eslint": "^8.30.0",
    "husky": "4",
    "jsdom": "^20.0.3",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.20",
    "rimraf": "^3.0.2",
    "standard-version": "^9.5.0",
    "tailwindcss": "^3.2.4",
    "typescript": "^4.9.4",
    "vite": "^4.0.3",
    "vite-plugin-top-level-await": "^1.2.2",
    "vite-plugin-wasm": "^3.1.0",
    "vite-tsconfig-paths": "^4.0.3",
    "vitest": "^0.26.2",
    "yarn-deduplicate": "^6.0.1"
  }
}
