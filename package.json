    "@headlessui/react": "^1.7.14",
    "@thi.ng/ecs": "^0.7.50",
    "bitecs": "^0.3.38",
    "box2d-wasm": "^7.0.0",
    "classnames": "^2.3.2",
    "extra-defer": "^0.2.7",
    "extra-ecs": "^0.5.0",
    "extra-game-loop": "^0.3.3",
    "extra-key-state": "^0.1.2",
    "extra-rand": "^0.1.4",
    "extra-react-hooks": "^0.5.2",
    "pixi.js": "^7.2.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "screenfull": "^6.0.2",
    "structure-of-arrays": "^0.8.0"
